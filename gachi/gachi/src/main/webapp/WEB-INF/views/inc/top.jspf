			}
		if($("#userpwd2").val()==""){
			alert("비밀번호를 입력하여주세요");
			$("#userpwd2").val(""); 
			$("#userpwd2").focus();
			return false;
			}
		if(!pwdChk.test($("#userpwd2").val())){ 
			alert("비밀번호 형식에 맞지않습니다(8자 이상16자리이하 영어,특수문자,숫자 조합)");
			$("#userpwd2").val(""); 
			$("#userpwd2").focus(); 
			return false;
			}
		if($("#userpwdChk").val()==""){
			alert("비밀번호확인을 입력하여주세요");
			$("#userpwdChk").val(""); 
			$("#userpwdChk").focus();
			return false;
			}
		if($("#userpwd2").val() != $("#userpwdChk").val()){
			alert("비밀번호가 같지않습니다 다시확인해주세요");
			$("#userpwdChk").val(""); 
			$("#userpwdChk").focus();
