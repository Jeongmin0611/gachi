	//로그인폼 아이디 비밀번호 유효성검사
    $("#loginEffectiveness").submit(function(){
		if($("#userid").val()==""){
			alert("아이디(이메일)을 입력하여주세요");
			$("#userid").val(""); 
			 $("#userid").focus();
			return false;
			}
