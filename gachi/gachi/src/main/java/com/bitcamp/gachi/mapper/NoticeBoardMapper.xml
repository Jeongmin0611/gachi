<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bitcamp.gachi.board.BoardDaoImp">
	<select id="noticeBoardAllRecord" resultType="com.bitcamp.gachi.board.NoticeBoardVO">
		select no, subject, writer, to_char(writedate, 'MM-DD') writedate 
		from notice order by no desc
	</select>
</mapper>